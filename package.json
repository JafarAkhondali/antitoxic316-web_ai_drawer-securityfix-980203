{
  "name": "ai_drawer",
  "version": "1.0.0",
  "description": "drawer with mnist evaluation",
  "main": "dist/index.js",
  "scripts": {
    "build-web": "browserify ./src/server.js -o ./dist/index.js -t [ babelify --presets [env] ]",
    "build-sketchPad": "browserify ./src/sketchPad.js -o --standalone SP > ./dist/bundled_sketchPad.js -t [ babelify --presets [env] ]",
    "build-cli": "browserify ./src/server.js --node -o ./dist/index.js -t [ babelify --presets [env] ]",
    "start": "npm run build-sketchPad && npm run build-cli && node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antitoxic316/web_ai_drawer.git"
  },
  "author": "antitoxic316",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antitoxic316/web_ai_drawer/issues"
  },
  "homepage": "https://github.com/antitoxic316/web_ai_drawer#readme",
  "devDependencies": {
    "@babel/preset-stage-1": "^7.8.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^15.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.5",
    "@tensorflow/tfjs-node": "3.18.0",
    "babel-cli": "^6.23.0",
    "encoding": "^0.1.13",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "path": "^0.12.7",
    "url": "^0.11.3"
  }
}
